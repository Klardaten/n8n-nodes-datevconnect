{
  "name": "@klardaten/n8n-nodes-datevconnect",
  "version": "1.0.3",
  "description": "n8n community node for DATEVconnect API via Klardaten Gateway",
  "license": "MIT",
  "homepage": "https://github.com/Klardaten/n8n-nodes-datevconnect#readme",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "klardaten",
    "datev",
    "datevconnect",
    "master-data",
    "document-management",
    "iam"
  ],
  "author": {
    "name": "Klardaten",
    "email": "info@klardaten.com",
    "url": "https://klardaten.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Klardaten/n8n-nodes-datevconnect.git"
  },
  "scripts": {
    "test": "bun test",
    "build": "npx n8n-node build && npm run build:types",
    "build:watch": "tsc --watch",
    "build:types": "tsc -p tsconfig.declarations.json",
    "dev": "npx n8n-node dev",
    "lint": "npx n8n-node lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "npx n8n-node lint --fix",
    "release": "npx n8n-node release",
    "prepublishOnly": "npx n8n-node prerelease"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/MasterData/MasterData.node.js",
      "dist/nodes/Accounting/Accounting.node.js",
      "dist/nodes/DocumentManagement/DocumentManagement.node.js",
      "dist/nodes/IdentityAndAccessManagement/IdentityAndAccessManagement.node.js"
    ],
    "credentials": [
      "dist/credentials/DatevConnectApi.credentials.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/Klardaten/n8n-nodes-datevconnect/issues"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^24.10.0",
    "bun": "^1.3.2",
    "bun-types": "^1.3.2",
    "eslint": "9.39.1",
    "nock": "14.0.10",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
